{
  "request": {
    "method": "GET",
    "urlPath": "/users",
    "queryParameters" : {
      "username" : {
        "matches" : "([A-Za-z]){1,10}"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "a-user.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}